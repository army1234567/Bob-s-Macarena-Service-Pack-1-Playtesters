// Core Classes
class Song {
  constructor(title, artist, coverLink, audioLink) {
    this.title = title;
    this.artist = artist;
    this.coverLink = coverLink;
    this.audioLink = audioLink;
  }
}

class Playlist {
  constructor(name) {
    this.name = name;
    this.songs = [];
    this.currentSongIndex = 0;
  }

  addSong(song) {
    this.songs.push(song);
  }

  getCurrentSong() {
    return this.songs[this.currentSongIndex];
  }

  playNext() {
    if (this.currentSongIndex < this.songs.length - 1) {
      this.currentSongIndex++;
    } else {
      this.currentSongIndex = this.songs.length - 1;
    }
  }

  playPrevious() {
    if (this.currentSongIndex > 0) {
      this.currentSongIndex--;
    } else {
      this.currentSongIndex = 0;
    }
  }
}

window.Playlist = Playlist;
window.Song = Song;

let playlists = [];
let currentPlaylist = null;
let loopSong = false;

function changePlaylist(newPlaylist) {
  currentPlaylist = newPlaylist;
  currentPlaylist.currentSongIndex = 0;
  updateUI();
  const audio = document.getElementById("audio");
  audio.src = currentPlaylist.getCurrentSong().audioLink;
  audio.play();
}
window.changePlaylist = changePlaylist;

// UI update
function updateUI() {
  const currentSong = currentPlaylist.getCurrentSong();
  document.getElementById("cover").src = currentSong.coverLink;
  document.getElementById("title").textContent = currentSong.title;
  document.getElementById("artist").textContent = currentSong.artist;
}

// Player setup
function setupMusicPlayer() {
  const defaultPlaylist = new Playlist("The Senator's Tumble");
  defaultPlaylist.addSong(new Song(
    "Macarena", "Los Del Rio ‚Äß 1993",
    "https://files.catbox.moe/gfggqq.png",
    "https://files.catbox.moe/msyl1k.mp3"
  ));

  const playlist1 = new Playlist("FM 90.1");
  playlist1.addSong(new Song(
    "I Believe I Can Fly", "R. Kelly ‚Äß 1997",
    "https://files.catbox.moe/xrwsna.png",
    "https://files.catbox.moe/0bvsll.mp3"
  ));
  playlist1.addSong(new Song(
    "I Don't Want to Miss a Thing", "Aerosmith ‚Äß 1998",
    "https://files.catbox.moe/c30ml3.png",
    "https://files.catbox.moe/q20nux.mp3"
  ));
  playlist1.addSong(new Song(
    "I Want It That Way", "Backstreet Boys ‚Äß 1999",
    "https://files.catbox.moe/dunr4h.png",
    "https://files.catbox.moe/n60qaj.mp3"
  ));
  playlist1.addSong(new Song(
    "I Want to Know What Love Is", "Foreigner ‚Äß 1999 (Remastered)",
    "https://files.catbox.moe/jp9cfh.png",
    "https://files.catbox.moe/gwsdcz.mp3"
  ));
  playlist1.addSong(new Song(
    "I Knew I Loved You", "Savage Garden ‚Äß 1999",
    "https://files.catbox.moe/kdhjpj.png",
    "https://files.catbox.moe/3tmsv7.mp3"
  ));
  playlist1.addSong(new Song(
    "Music", "Madonna ‚Äß 2000",
    "https://files.catbox.moe/v82x55.png",
    "https://files.catbox.moe/lioj4r.mp3"
  ));
  playlist1.addSong(new Song(
    "Smooth", "Santana (feat. Rob Thomas) ‚Äß 1999",
    "https://files.catbox.moe/et28xr.png",
    "https://files.catbox.moe/zyr4bv.mp3"
  ));  
  playlist1.addSong(new Song(
    "My Heart Will Go On", "Celine Dion ‚Äß 1997",
    "https://files.catbox.moe/92u8q0.png",
    "https://files.catbox.moe/xrwax1.mp3"
  ));  

  const playlist2 = new Playlist("FM 93.1");
  playlist2.addSong(new Song(
    "Paranoid Android", "Radiohead ‚Äß 1997",
    "https://files.catbox.moe/dj4eo9.png",
    "https://files.catbox.moe/9pn2nn.mp3"
  ));
  playlist2.addSong(new Song(
    "99 Red Balloons", "Goldfinger ‚Äß 2000",
    "https://files.catbox.moe/e9shkj.png",
    "https://files.catbox.moe/v4wxcn.mp3"
  ));
  playlist2.addSong(new Song(
    "Breathe", "The Prodigy ‚Äß 1996",
    "https://files.catbox.moe/tncm0b.png",
    "https://files.catbox.moe/sddnkz.mp3"
  ));
  playlist2.addSong(new Song(
    "Block Rockin' Beats", "The Chemical Brothers ‚Äß 1997",
    "https://files.catbox.moe/5zx5rm.png",
    "https://files.catbox.moe/8s0sbi.mp3"
  ));
  playlist2.addSong(new Song(
    "Firestarter", "The Prodigy ‚Äß 1997",
    "https://files.catbox.moe/w8rdc6.png",
    "https://files.catbox.moe/6roskn.mp3"
  ));
  playlist2.addSong(new Song(
    "Papercut", "Linkin Park ‚Äß 2000",
    "https://files.catbox.moe/ebriii.png",
    "https://files.catbox.moe/bbg3ho.mp3"
  ));
  playlist2.addSong(new Song(
    "Ocean Man", "Ween ‚Äß 1997",
    "https://files.catbox.moe/a4xxjh.png",
    "https://files.catbox.moe/rz74mh.mp3"
  ));

  // Add playlists to the array
  playlists.push(defaultPlaylist);
  playlists.push(playlist1);
  playlists.push(playlist2);


  currentPlaylist = defaultPlaylist;

  const player = document.createElement("div");
  player.id = "player98";

  const titleBar = document.createElement("div");
  titleBar.className = "title-bar";
  titleBar.innerHTML = `<div class="title-bar-text">Windows Media Player</div>
                        <div class="title-bar-controls"><button aria-label="Minimize"></button><button aria-label="Close"></button></div>`;
  player.appendChild(titleBar);

  const display = document.createElement("div");
  display.id = "display";
  display.innerHTML = `
    <img id="cover" src="${currentPlaylist.getCurrentSong().coverLink}" />
    <div id="info">
      <div id="title">${currentPlaylist.getCurrentSong().title}</div>
      <div id="artist">${currentPlaylist.getCurrentSong().artist}</div>
    </div>`;
  player.appendChild(display);

  const controls = document.createElement("div");
  controls.id = "controls";
  controls.innerHTML = `
    <button id="prev">‚óÑ‚óÑ</button>
    <button id="playPause">‚ñ∂</button>
    <button id="next">‚ñ∫‚ñ∫</button>
    <button id="loop">‚Üª</button>
  `;
  player.appendChild(controls);

  const progressContainer = document.createElement("div");
  progressContainer.id = "progressContainer";
  const progressFill = document.createElement("div");
  progressFill.id = "progressFill";
  progressContainer.appendChild(progressFill);
  player.appendChild(progressContainer);

  const playlistSelector = document.createElement("select");
  playlistSelector.id = "playlistSelector";
  playlists.forEach((pl, idx) => {
    const opt = document.createElement("option");
    opt.value = idx;
    opt.textContent = pl.name;
    playlistSelector.appendChild(opt);
  });
  player.appendChild(playlistSelector);

  const volumeLabel = document.createElement("div");
  volumeLabel.id = "volumeLabel";
  volumeLabel.textContent = "Volume:";
  player.appendChild(volumeLabel);

  const volume = document.createElement("input");
  volume.type = "range";
  volume.id = "volume";
  volume.min = 0;
  volume.max = 1;
  volume.step = 0.1;
  volume.value = 1;
  player.appendChild(volume);

  const audio = document.createElement("audio");
  audio.id = "audio";
  audio.src = currentPlaylist.getCurrentSong().audioLink;
  document.body.appendChild(audio);

  const gameWindow = document.getElementById("game_window");
  if (gameWindow) {
    gameWindow.insertAdjacentElement("afterend", player);
  } else {
    document.body.appendChild(player);
  }

  document.getElementById("playPause").addEventListener("click", () => {
    if (audio.paused) {
      audio.play();
      document.getElementById("playPause").textContent = "‚ùö‚ùö";
    } else {
      audio.pause();
      document.getElementById("playPause").textContent = "‚ñ∂";
    }
  });

  document.getElementById("next").addEventListener("click", () => {
    currentPlaylist.playNext();
    updateUI();
    audio.src = currentPlaylist.getCurrentSong().audioLink;
    audio.play();
  });

  document.getElementById("prev").addEventListener("click", () => {
    currentPlaylist.playPrevious();
    updateUI();
    audio.src = currentPlaylist.getCurrentSong().audioLink;
    audio.play();
  });

  // üîÅ Loop toggle with icons
  document.getElementById("loop").addEventListener("click", () => {
    loopSong = !loopSong;
    audio.loop = loopSong;
    document.getElementById("loop").textContent = loopSong ? "‚áÜ" : "‚Üª";
  });

  playlistSelector.addEventListener("change", (e) => {
    changePlaylist(playlists[e.target.value]);
  });

  progressContainer.addEventListener("click", (e) => {
    const rect = progressContainer.getBoundingClientRect();
    const percent = (e.clientX - rect.left) / rect.width;
    audio.currentTime = percent * audio.duration;
  });

  audio.addEventListener("timeupdate", () => {
    const percent = (audio.currentTime / audio.duration) * 100;
    progressFill.style.width = percent + "%";
  });

  volume.addEventListener("input", () => {
    audio.volume = volume.value;
  });

  audio.addEventListener("ended", () => {
    if (!loopSong) {
      currentPlaylist.playNext();
      updateUI();
      audio.src = currentPlaylist.getCurrentSong().audioLink;
      audio.play();
    }
  });

  audio.play().then(() => {
    document.getElementById("playPause").textContent = "‚ùö‚ùö";
  }).catch(err => {
    console.log("Autoplay blocked by browser:", err);
  });
}

