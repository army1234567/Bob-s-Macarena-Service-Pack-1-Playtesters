            `)}

        </div>
    `;

    document.body.appendChild(customWindowContainer);
    makeDraggable(customWindowContainer);

    // Attach event listeners for section toggle buttons
    document.querySelectorAll(".toggle-section").forEach(button => {
        button.addEventListener("click", function () {
            const content = document.getElementById(this.dataset.target);
            content.style.display = content.style.display === "none" ? "block" : "none";
            this.innerHTML = content.style.display === "none" ? "[ + ]" : "[ - ]";
        });
    });
}

function createSection(title, id, content) {
    return `
        <div class="section" style="margin-bottom: 10px; border-bottom: 2px solid #7C9CCC;">
            <div class="header-box" style="background-color: #7C9CCC; padding: 5px; display: flex; justify-content: space-between; align-items: center;">
                <h3 style="color: white; font-size: 14px; margin: 0;">${title}</h3>
                <button class="toggle-section" data-target="${id}" style="
                    background-color: #c0c0c0;
                    border: 2px outset #808080;
                    font-family: 'MS Sans Serif';
                    font-size: 12px;
                    cursor: pointer;
                    padding: 2px 5px;
                ">[ + ]</button>
            </div>
            
            <div id="${id}" class="content-layer" style="background-color: #f0f0f0; padding: 10px; display: none; border: 2px solid #c0c0c0;">
                ${content}
            </div>
        </div>
    `;
}

function makeDraggable(element) {
    let dragHandle = element.querySelector("#drag-handle");
    dragHandle.addEventListener("mousedown", (e) => {
        let offsetX = e.clientX - element.offsetLeft;
        let offsetY = e.clientY - element.offsetTop;
        
        function onMouseMove(e) {
            element.style.left = `${e.clientX - offsetX}px`;
            element.style.top = `${e.clientY - offsetY}px`;
        }

        function onMouseUp() {
            document.removeEventListener("mousemove", onMouseMove);
            document.removeEventListener("mouseup", onMouseUp);
        }

        document.addEventListener("mousemove", onMouseMove);
        document.addEventListener("mouseup", onMouseUp);
    });
}

function addDynamicCSS() {
    var css = `
@keyframes tooltipFlash {
    0% {
        opacity: 0;
        transform: scale(0.8);
    }
    50% {
        opacity: 1;
        transform: scale(1.05);
    }
    100% {
        opacity: 1;
        transform: scale(1);
    }
}

.mytooltip {
    position: relative;
    display: inline-block;
    cursor: pointer;
    text-decoration: underline;
    text-decoration-color: red;
    text-decoration-thickness: 2px;
}

.mytooltip .mytooltiptext {
    background-color: white;
    color: black;
    text-align: center;
    border-radius: 6px;
    padding: 10px;
    position: absolute;
    z-index: 99;
    bottom: 20%;
    left: 20%;
    margin-left: -90px;
    opacity: 0;
    transition: opacity 0.3s;
    border: 1px solid black;
    pointer-events: none;
    font-size: 13px !important;
    line-height: 1.5em !important;
    font-weight: normal !important;
    font-style: normal !important;
    box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.2);
    display: flex;
    flex-direction: column;
    align-items: center;
    width: fit-content;
    max-width: 600px;
    transform: scale(0.95);
}

.mytooltip .mytooltiptext img {
    max-width: 200px;
    height: auto;
}

.mytooltip:hover .mytooltiptext {
    opacity: 1;
    animation: tooltipFlash 0.4s ease-in-out forwards;
    transition-delay: 0.2s;
    width: calc(1.5 * 200px);
}
    `;

    var styleElem = document.createElement('style');
    styleElem.type = 'text/css';
    
    styleElem.appendChild(document.createTextNode(css));
    
    document.head.appendChild(styleElem);
}

addDynamicCSS();


